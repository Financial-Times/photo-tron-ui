{
  "name": "photo-tron-ui",
  "version": "1.0.0",
  "description": "A proof-of-concept for a possible revamp of photo-tron-ui Charts, using victory.js/D3",
  "scripts": {
    "build": "npm run clean && webpack --config webpack.config.js",
    "clean": "rm -rf _build",
    "coverage": "jest --coverage",
    "lint": "eslint -c .eslintrc src",
    "performance": "mkdir test/artillery/reports && artillery run test/artillery/performance.yml -e staging -o test/artillery/reports/artillery_report.json && artillery report test/artillery/reports/artillery_report.json",
    "postinstall": "npm run build",
    "production": "babel-node server.js",
    "start": "npm build && npm run production",
    "test": "npm run unit",
    "unit": "NODE_ENV=unit jest --config .jest --runInBand --no-cache",
    "unit-watch": "NODE_ENV=unit jest --no-cache --config .jest --watch",
    "precommit": "npm run unit && npm run lint && node_modules/.bin/secret-squirrel",
    "commitmsg": "node_modules/.bin/secret-squirrel-commitmsg"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Financial-Times/photo-tron-ui.git"
  },
  "keywords": [
    "photo-tron-ui",
    "Victory",
    "React",
    "MobX"
  ],
  "author": "Yolande Leung, Luca Panziera, Andrea Medeghini, Ashoor Namrood",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Financial-Times/photo-tron-ui/issues"
  },
  "homepage": "https://github.com/Financial-Times/photo-tron-ui#readme",
  "engines": {
    "node": "~6.11.1"
  },
  "devDependencies": {
    "@financial-times/secret-squirrel": "^2.5.10",
    "artillery": "^1.6.0-0",
    "babel-eslint": "^6.1.2",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-typecheck": "^3.9.0",
    "cucumber": "^2.0.0-rc.9",
    "eslint": "^3.16.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-react": "^6.10.0",
    "husky": "^0.14.3",
    "nightwatch": "^0.9.15",
    "nightwatch-cucumber": "^7.1.2",
    "nightwatch-cucumber-editor": "^1.0.1",
    "phantomjs-prebuilt": "^2.1.14",
    "selenium-server": "^3.4.0",
    "webpack-dev-middleware": "^1.11.0"
  },
  "dependencies": {
    "@financial-times/express-web-service": "^3.3.1",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-class-properties": "^6.22.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.3.13",
    "body-parser": "^1.18.1",
    "css-loader": "^0.26.1",
    "dotenv": "^4.0.0",
    "express": "^4.14.1",
    "express-session": "^1.15.2",
    "extract-text-webpack-plugin": "^2.0.0-beta",
    "html-webpack-plugin": "^2.28.0",
    "jest-cli": "^18.1.0",
    "lodash": "^4.17.4",
    "material-ui": "^1.0.0-beta.11",
    "material-ui-icons": "^1.0.0-beta.10",
    "mobx": "^3.1.0",
    "mobx-react": "^4.1.0",
    "moment": "^2.17.1",
    "newrelic": "^1.39.0",
    "node-sass": "^4.5.0",
    "postcss-loader": "^1.2.2",
    "prop-types": "^15.5.10",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-router": "^3.0.5",
    "react-tap-event-plugin": "^2.0.1",
    "react-tools": "^0.13.3",
    "react-transform": "0.0.3",
    "s3o-middleware": "^1.8.0",
    "sass-loader": "^5.0.1",
    "style-loader": "^0.13.1",
    "superagent": "^3.4.4",
    "victory": "^0.27.2",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^1.16.2",
    "webpack-hot-middleware": "^2.16.1"
  }
}
